{
  "name": "api",
  "root": "apps/api",
  "sourceRoot": "apps/api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/run-gradle.js build -x test"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/run-gradle.js bootRun"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/run-gradle.js test"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/run-gradle.js spotlessApply"
      }
    }
  }
}
